{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@andyfangdz/prettierrc": "^6.0.1",
    "nightmare": "^3.0.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "serve": "^10.0.2"
  },
  "dependencies": {
    "autotrack": "^2.4.1",
    "classnames": "^2.2.6",
    "fontfaceobserver": "^2.0.13",
    "history": "^4.6.3",
    "material-ui": "1.0.0-beta.37",
    "material-ui-icons": "1.0.0-beta.36",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-markdown": "^4.0.1",
    "react-scripts": "2.0.4",
    "react-tap-event-plugin": "^3.0.3",
    "route-parser": "^0.0.5"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar src/styles -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar -w src/styles -o src/styles",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
