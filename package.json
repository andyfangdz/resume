{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@andyfangdz/prettierrc": "^4.1.1",
    "nightmare": "^3.0.1",
    "node-sass": "^4.8.3",
    "node-sass-chokidar": "1.2.2",
    "npm-run-all": "^4.1.1",
    "serve": "^6.5.3"
  },
  "dependencies": {
    "autotrack": "^2.4.1",
    "classnames": "^2.2.5",
    "fontfaceobserver": "^2.0.13",
    "history": "^4.6.3",
    "material-ui": "1.0.0-beta.37",
    "material-ui-icons": "1.0.0-beta.36",
    "mobx": "^4.0.0",
    "mobx-react": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.3.0",
    "react-scripts": "1.1.1",
    "react-tap-event-plugin": "^3.0.0",
    "route-parser": "^0.0.5",
    "source-map-explorer": "^1.5.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass src/styles -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar -w src/styles -o src/styles",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
